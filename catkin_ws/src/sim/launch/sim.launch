		<param name="q_imu_auv_y" value="$(arg q_imu_auv_y)" />
		<param name="q_imu_auv_z" value="$(arg q_imu_auv_z)" />

		<param name="q_dvl_auv_w" value="$(arg q_dvl_auv_w)" />
		<param name="q_dvl_auv_x" value="$(arg q_dvl_auv_x)" />
